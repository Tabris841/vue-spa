{
  "name": "vue-spa",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "node server",
    "dev": "rimraf ./dist && cross-env NODE_ENV=development node server",
    "debug": "rimraf ./dist && cross-env NODE_ENV=development node --nolazy --inspect-brk=9229 server",
    "test": "karma start test/unit/karma.config.js --single-run",
    "build:client": "cross-env NODE_ENV=production webpack --config ./build/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config ./build/webpack.server.config.js --progress --hide-modules",
    "build": "rimraf ./dist && npm run build:client && npm run build:server",
    "start:prod": "cross-env NODE_ENV=production npm run start"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bulma": "^0.6.2",
    "express": "^4.16.3",
    "serialize-javascript": "^1.4.0",
    "vue": "^2.5.16",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.16",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.2",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "karma": "^2.0.0",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sinon-chai": "^1.3.3",
    "karma-webpack": "^3.0.0",
    "memory-fs": "^0.4.1",
    "mini-css-extract-plugin": "^0.4.0",
    "mocha": "^5.0.5",
    "node-sass": "^4.8.3",
    "prettier": "1.11.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.7",
    "sinon": "^4.5.0",
    "sinon-chai": "^3.0.0",
    "vue-loader": "^14.2.2",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.4.1",
    "webpack-dev-middleware": "^3.1.0",
    "webpack-hot-middleware": "^2.21.2",
    "webpack-node-externals": "^1.7.2"
  }
}
